<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Scheduled_Date_Cannot_Be_Past</fullName>
    <active>true</active>
    <errorConditionFormula>Scheduled_Date__c &lt; NOW()</errorConditionFormula>
    <errorDisplayField>Scheduled_Date__c</errorDisplayField>
    <errorMessage>Scheduled date cannot be in the past.</errorMessage>
</ValidationRule>
