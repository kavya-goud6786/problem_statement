<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsLate_c__c</fullName>
    <formula>AND(
   NOT(ISBLANK(Actual_Delivery_Time__c)),
   Actual_Delivery_Time__c &gt; Scheduled_Date__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>IsLate__c</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
